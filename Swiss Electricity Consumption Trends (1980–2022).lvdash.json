{
  "datasets": [
    {
      "name": "58b4ad39",
      "displayName": "electricity_annual_trend",
      "queryLines": [
        "SELECT\r\n",
        "  year,\r\n",
        "  ROUND(SUM(tj_sum) / 3600.0, 1) AS electricity_twh\r\n",
        "FROM workspace.default.gold_energy_annual\r\n",
        "WHERE energy_carrier = 'electricity'\r\n",
        "GROUP BY year\r\n",
        "ORDER BY year;\r\n",
        "\r\n",
        "\r\n"
      ]
    },
    {
      "name": "3afd23df",
      "displayName": "YoY change (%) для Electricity",
      "queryLines": [
        "WITH base AS (\r\n",
        "  SELECT\r\n",
        "    year,\r\n",
        "    SUM(tj_sum) / 3600.0 AS electricity_twh\r\n",
        "  FROM workspace.default.gold_energy_annual\r\n",
        "  WHERE energy_carrier = 'electricity'\r\n",
        "  GROUP BY year\r\n",
        "),\r\n",
        "yoy AS (\r\n",
        "  SELECT\r\n",
        "    year,\r\n",
        "    electricity_twh,\r\n",
        "    (electricity_twh - LAG(electricity_twh) OVER (ORDER BY year))\r\n",
        "      / LAG(electricity_twh) OVER (ORDER BY year) * 100 AS yoy_pct\r\n",
        "  FROM base\r\n",
        ")\r\n",
        "SELECT\r\n",
        "  year,\r\n",
        "  yoy_pct\r\n",
        "FROM yoy\r\n",
        "WHERE yoy_pct IS NOT NULL\r\n",
        "ORDER BY year;\r\n"
      ]
    },
    {
      "name": "248fd7dc",
      "displayName": "Electricity Growth Index Since 1980",
      "queryLines": [
        "WITH base AS (\r\n",
        "  SELECT\r\n",
        "    year,\r\n",
        "    SUM(tj_sum) / 3600.0 AS electricity_twh\r\n",
        "  FROM workspace.default.gold_energy_annual\r\n",
        "  WHERE energy_carrier = 'electricity'\r\n",
        "  GROUP BY year\r\n",
        "),\r\n",
        "base_year AS (\r\n",
        "  SELECT electricity_twh AS base_value\r\n",
        "  FROM base\r\n",
        "  WHERE year = 1980\r\n",
        ")\r\n",
        "SELECT\r\n",
        "  b.year,\r\n",
        "  (b.electricity_twh / by.base_value) * 100 AS index_1980_100\r\n",
        "FROM base b\r\n",
        "CROSS JOIN base_year by\r\n",
        "ORDER BY b.year;\r\n"
      ]
    }
  ],
